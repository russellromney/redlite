{
  "name": "redlite",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "TypeScript SDK for Redlite - Redis API + SQLite durability",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "redlite",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js",
    "universal": "napi universal"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "redlite",
    "redis",
    "database",
    "embedded",
    "sqlite",
    "typescript",
    "native"
  ],
  "author": "Redlite Contributors",
  "license": "MIT"
}
